[alias]
  st = status -s
  ci = commit
  cim = commit -m
  co = checkout
  pu = pull upstream master
  d = diff
[user]
  name = Viktor Nonov
  email = vnonov@gmail.com
[core]
  editor = /usr/bin/vim
[push]
  default = simple
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red
  new = green
[color "status"]
  added = green
  changed = yellow
  untracked = red
